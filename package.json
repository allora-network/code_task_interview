{
  "name": "coding-task-interview",
  "version": "1.0.0",
  "description": "Coding interview task with Kafka, NextJS, and Postgres",
  "scripts": {
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:down": "npx sequelize-cli db:migrate:undo",
    "db:migration:create": "npx sequelize-cli migration:generate --name",
    "consumer": "node backend/consumer/index.js",
    "send:event": "ts-node script/original_event.ts"
  },
  "dependencies": {
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/node": "^20.10.5"
  }
}
